<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

     <extension
         id="org.xtuml.ui.x2m.Export"
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            id="org.xtuml.bp.x2m.contribution1"
            objectClass="org.xtuml.bp.core.SystemModel_c">
         <action
               class="org.xtuml.bp.x2m.actions.ExportProjectAction"
               enablesFor="1"
               id="org.xtuml.bp.x2m.ExportProjectAction"
               label="Export MASL project"
               menubarPath="org.xtuml.bp.ui.context-internal">
         </action>
      </objectContribution>
      <objectContribution
            id="org.xtuml.bp.x2m.contribution2"
            objectClass="org.xtuml.bp.core.SystemModel_c">
         <action
               class="org.xtuml.bp.x2m.actions.ExportDomainAction"
               enablesFor="1"
               id="org.xtuml.bp.x2m.ExportDomainAction"
               label="Export MASL domain"
               menubarPath="org.xtuml.bp.ui.context-internal">
         </action>
      </objectContribution>
   </extension>
  <extension point="org.eclipse.ant.core.antTasks">
    <antTask
          class="org.xtuml.bp.x2m.ant.tasks.ExportProjectTask"
          eclipseRuntime="true"
          headless="true"
          library="x2m.jar"
          name="masl.ProjectExporter"/>
    <antTask
          class="org.xtuml.bp.x2m.ant.tasks.ExportDomainTask"
          eclipseRuntime="true"
          headless="true"
          library="x2m.jar"
          name="masl.DomainExporter"/>
  </extension>
</plugin>
